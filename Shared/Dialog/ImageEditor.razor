@inject IJSRuntime Js
<MudDialog>
    <DialogContent>
        <MudTextField @bind-Value="url"></MudTextField>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Outlined" OnClick="InsertByUrl">插入</MudButton>
    </DialogActions>
</MudDialog>
@code {
    string url = "";
    async void InsertByUrl()
    {
        var content = await Js.InvokeAsync<string>("GetEditorData");
        await Js.InvokeVoidAsync("SetEditorData",content+$"<img src='{url}' />");
    }
}