@inject IJSRuntime Js
<MudDialog>
    <DialogContent>
        <MudTextField @bind-Value="musicFile"></MudTextField>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Outlined" OnClick="InsertByUrl">插入</MudButton>
    </DialogActions>
</MudDialog>
@code {
    string musicFile = "";
    async void InsertByUrl()
    {
        var content = await Js.InvokeAsync<string>("GetEditorData");
        Console.WriteLine( content + $"<audio src={musicFile} controls='controls'>");
        await Js.InvokeVoidAsync("SetEditorData", content + $"<audio src={musicFile} controls='controls'>");
    }
}